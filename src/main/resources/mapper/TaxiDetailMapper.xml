<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cindy.ocrdemo.mapper.TaxiDetailMapper">

    <resultMap id="BaseResultMap" type="com.cindy.ocrdemo.pojo.TaxiDetail">
            <id property="taxiId" column="taxi_id" jdbcType="BIGINT"/>
            <result property="taxiDaima" column="taxi_daima" jdbcType="VARCHAR"/>
            <result property="taxiHaoma" column="taxi_haoma" jdbcType="VARCHAR"/>
            <result property="carNo" column="car_no" jdbcType="VARCHAR"/>
            <result property="taxiDate" column="taxi_date" jdbcType="DATE"/>
            <result property="taxiTime" column="taxi_time" jdbcType="VARCHAR"/>
            <result property="amount" column="amount" jdbcType="DECIMAL"/>
            <result property="price" column="price" jdbcType="DECIMAL"/>
            <result property="mileage" column="mileage" jdbcType="VARCHAR"/>
            <result property="note" column="note" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        taxi_id,taxi_daima,taxi_haoma,
        car_no,taxi_date,taxi_time,
        amount,price,mileage,
        note
    </sql>
</mapper>
